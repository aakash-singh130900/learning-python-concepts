    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "My name is: Thor\n",
      "My name is: Thor\n"
     ]
    }
   ],
   "source": [
    "#Introduced in Python 3.6, f-strings offer several benefits over the older .format() string method described above. \n",
    "\n",
    "name = 'Thor'\n",
    "\n",
    "# Using f-string\n",
    "print(f\"My name is: {name}\")\n",
    "\n",
    "# Using .format()\n",
    "print(\"My name is: {}\".format(name))\n",
    "\n"
   ]
